                android:padding="3dp"
                android:text=""
                android:textSize="7pt" />

            <TextView
                android:layout_column="1"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:layout_margin="0.5dip"
                android:background="#D0D0D0"
                android:padding="3dip"
                android:text="深度(cm)"
                android:textSize="7pt" />

            <TextView
                android:layout_column="1"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:layout_margin="0.5dip"
                android:background="#D0D0D0"
                android:padding="3dip"
                android:text="按壓頻率"
                android:textSize="7pt" />

            <TextView
                android:layout_column="1"
                android:layout_width="0dp"
